{
  "name": "vscode-language-jsonata-enhanced",
  "displayName": "VSCode Language JSONata Enhanced",
  "description": "Language support for JSONata with enhanced support for syntax highlighting - A fork of the original vscode-language-jsonata extension",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.64.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danmactough/vscode-language-jsonata.git"
  },
  "bugs": {
    "url": "https://github.com/danmactough/vscode-language-jsonata/issues"
  },
  "author": {
    "name": "Dan MacTough",
    "url": "https://github.com/danmactough"
  },
  "contributors": [
    {
      "name": "@bigbug",
      "url": "https://github.com/bigbug"
    }
  ],
  "homepage": "https://github.com/danmactough/vscode-language-jsonata",
  "license": "MIT",
  "icon": "images/jsonata-button.png",
  "publisher": "danmactough",
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "keywords": [
    "jsonata",
    "autocomplete",
    "syntax",
    "highlight",
    "notebook",
    "linter"
  ],
  "activationEvents": [
    "onNotebook:jsonata-book",
    "onLanguage:jsonata"
  ],
  "main": "./dist/extension.js",
  "browser": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "jsonata",
        "extensions": [
          ".jsonata"
        ],
        "configuration": "./syntaxes/jsonata-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jsonata",
        "scopeName": "source.jsonata",
        "path": "./syntaxes/jsonata.tmLanguage.json"
      }
    ],
    "notebooks": [
      {
        "id": "jsonata-book",
        "type": "jsonata-book",
        "displayName": "JSONata Book",
        "selector": [
          {
            "filenamePattern": "*.jsonata-book"
          }
        ]
      }
    ],
    "configuration": {
      "title": "JSONata Enhanced",
      "properties": {
        "jsonata.formatterEnabled": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable JSONata formatter (experimental)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p . --outDir out",
    "pretest": "npm run test-compile && npm run compile && npm run lint",
    "lint": "eslint --ext ts,json .",
    "lint:fix": "eslint --ext ts,json --fix .",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/jsonata": "^1.5.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "@vscode/test-web": "*",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-json": "^4.0.1",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "json-stringify-safe": "^5.0.1",
    "jsonata": "^2.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "vscode-uri": "^3.0.3",
    "xml2js": "^0.6.2"
  }
}
